<html>
<head>
    <style>
        .container {
            padding-right: 15px;
            padding-left: 15px;
            margin-right: auto;
            margin-left: auto
        }
        .well {
            min-height: 20px;
            padding: 19px;
            margin-bottom: 20px;
            background-color: #f5f5f5;
            border: 1px solid #e3e3e3;
            border-radius: 4px;
            -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
            box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05)
        }
        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            text-align: center;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #f2f2f2
        }

        th {
            background-color: #4CAF50;
            color: white;
            min-width: 150px;
            text-align: center;
        }
    </style>
</head>
<body>
<div class="container well" style="margin-top: 50px">
    <p style="text-align: center; font-size: 36px;">Liste de Vos Projets</p>
    <table id="tablee">
        <tr>
            <th>Nom</th>
            <th>Description</th>
            <th>Date DÃ©but</th>
            <th>Date Fin</th>
            <th>Chef du Projet</th>
        </tr>
        {% for projet in projets %}
            <tr>
                <td>{{ projet.nom }}</td>
                <td>{{ projet.description }}</td>
                <td>{{ projet.dateDebut|date('d - F - Y') }}</td>
                <td>{{ projet.dateFin|date('d - F - Y') }}</td>
                <td>{{ projet.chefProjet.nom }}</td>
            </tr>
        {% endfor %}
    </table>
</div>
</body>
</html>

